{
    "compilerOptions": {
      "forceConsistentCasingInFileNames": true,
      "jsx": "react-jsx",
      "module": "commonjs",
      "noImplicitAny": true,
      "noImplicitThis": true,
      "outDir": "dist/scripts",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es6",
      "esModuleInterop": true
    },
    "include": [
      "App/index.tsx"
    ]
  }